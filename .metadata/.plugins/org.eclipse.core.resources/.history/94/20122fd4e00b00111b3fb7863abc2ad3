package senha.core;

import java.util.List;

public class Retorno {

	private List<Integer>[] jogadasAnteriores;
	private List<Integer> retorno;

	public void imprimirResultado() {
		for (int i = 0; i < jogadasAnteriores.length; i++) {
			System.out.print(i + ": ");
			for (int j = 0; j < jogadasAnteriores[i].size(); j++) {
				System.out.print("| " + jogadasAnteriores[i].get(j));
			}
			System.out.println();
		}
		
		for(int i = 0 ; i < retorno.size() ; i++){
			
			
		}
		
	}

	public List<Integer> getRetorno() {
		return retorno;
	}

	public void setRetorno(List<Integer> retorno) {
		this.retorno = retorno;
	}
}
