

import arvore.Position;
import arvore.Tree;
import arvore.binaria.BinaryTree;
import arvore.lista.InvalidPositionException;

public interface AlvoArvoreGenerica<T> extends Tree<T>{
	public Iterable<Position<T>> primos (Position<T> p) throws CoursinNotFoundException;
	public Iterable<Position<T>> herdeiros (Position<T> p);
}
