package game.data;

public class Elemento {

	private final String nome;
	private final String chave;

	public Elemento(String nome, String chave) {
		super();
		this.nome = nome;
		this.chave = chave;
	}

	public String getNome() {
		return nome;
	}

	public String getChave() {
		return chave;
	}

	@Override
	public String toString() {
		return nome;
	}

}
