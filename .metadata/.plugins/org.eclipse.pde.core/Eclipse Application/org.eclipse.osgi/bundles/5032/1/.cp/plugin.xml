   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="org.eclipse.mtj.internal.ui.launching.MidletLaunchShortcut"
            icon="icons/full/obj16/cellphone_icon.gif"
            id="org.eclipse.mtj.launchShortcuts"
            label="%launch.emulator.shortcut"
            modes="run,debug">
            
         <contextualLaunch>
            <enablement>
               <with variable="selection">
                  <count value="1" />
               		<iterate>
		             <and>
		               <adapt type="org.eclipse.jdt.core.IJavaElement">
		                 <test property="org.eclipse.jdt.launching.hasProjectNature" args="org.eclipse.mtj.core.nature"/>
		               </adapt>
		               <or>
		                 <test property="org.eclipse.jdt.launching.extendsClass" args="javax.microedition.midlet.MIDlet"/>
		               	 <test property="org.eclipse.jdt.launching.isContainer"/>
		               	 <test property="org.eclipse.jdt.launching.isPackageFragment"/>
		               	 <test property="org.eclipse.jdt.launching.isPackageFragmentRoot"/>
		               </or>
		             </and>
               		</iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <description
               description="%launch.emulator.shortcut.description"
               mode="run">
         </description>
         <description
               description="%debug.emulator.shortcut.description"
               mode="debug">
         </description>
      </shortcut>
      <shortcut
            label="%launch.emulator.shortcut.jad"
            icon="icons/full/obj16/cellphone_icon.gif"
            modes="run,debug"
            class="org.eclipse.mtj.internal.ui.launching.JadLaunchShortcut"
            id="org.eclipse.mtj.jadLaunchShortcuts">
         <contextualLaunch>
            <enablement>
               <with variable="selection">
                  <count value="1" />
               		<iterate>
		             <or>
		              <adapt type="org.eclipse.core.resources.IFile">
		                  <and>
			                 <test property="org.eclipse.core.resources.projectNature" value="org.eclipse.mtj.core.nature"/>
			                 <test property="org.eclipse.core.resources.name" value="Application Descriptor"/>
		                  </and>
		              </adapt>
			          <adapt type="org.eclipse.core.resources.IProject">
			          	<test property="org.eclipse.core.resources.projectNature" value="org.eclipse.mtj.core.nature"/>
			          </adapt>
		             </or>
               	    </iterate>
               </with>
            </enablement>
         </contextualLaunch>
         <description
               description="%launch.emulator.shortcut.jad.description"
               mode="run">
         </description>
         <description
               description="%debug.emulator.shortcut.jad.description"
               mode="debug">
         </description>
         
      </shortcut>
      <shortcut
            class="org.eclipse.mtj.internal.ui.launching.OTALaunchShortcut"
            icon="icons/full/obj16/cellphone_icon.gif"
            id="org.eclipse.mtj.OTALaunchShortcuts"
            label="%launch.emulator.shortcut.OTA"
            modes="run,debug">
      </shortcut>
   </extension>
