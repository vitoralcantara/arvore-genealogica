   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.javaProject">
         <filter
               name="projectNature"
               value="org.eclipse.jdt.core.javanature">
         </filter>
         <menu
               label="%menu.project.j2me"
               path="additions"
               id="org.eclipse.mtj.ui.popup.menu">
            <separator
                  name="dummy">
            </separator>
            <separator
                  name="group">
            </separator>
         </menu>
       </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.j2meProject">
         <menu
               label="%menu.project.j2me"
               path="additions"
               id="org.eclipse.mtj.ui.popup.menu">
            <separator name="convert" />
            <separator name="package" />
            <separator name="export" />
         </menu>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.j2meProject">
         	<visibility>
         		<and>
					<objectState name="nature" value="org.eclipse.mtj.core.nature"/>
					<not>
						<objectState name="nature" value="org.eclipse.mtj.core.preprocessedNature"/>
	   				</not>
	   			</and>
  			</visibility>
         <action
               label="%menu.j2me.createObfuscatedPackage"
               class="org.eclipse.mtj.internal.ui.actions.packaging.CreateObfuscatedPackageAction"
               menubarPath="org.eclipse.mtj.ui.popup.menu/package"
               id="org.eclipse.mtj.ui.actions.createobfuscatedpackage">
         </action>
         <action
               label="%menu.j2me.createPackage"
               class="org.eclipse.mtj.internal.ui.actions.packaging.CreatePackageAction"
               menubarPath="org.eclipse.mtj.ui.popup.menu/package"
               id="org.eclipse.mtj.ui.actions.createpackage">
         </action>
         <action
               label="%menu.j2me.exportAntenna"
               class="org.eclipse.mtj.internal.ui.actions.exporting.AntennaBuildExportAction"
               menubarPath="org.eclipse.mtj.ui.popup.menu/export"
               id="org.eclipse.mtj.ui.actions.antennaexport">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.javaProject.convert">
			<visibility>
				<not>
					<objectState name="nature" value="org.eclipse.mtj.core.nature"/>
   				</not>
  			</visibility>
         <action
               label="%menu.j2me.convertProject"
               class="org.eclipse.mtj.internal.ui.actions.ConvertToMidletProjectAction"
               menubarPath="org.eclipse.mtj.ui.popup.menu/convert"
               id="org.eclipse.mtj.ui.actions.convertproject">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.project.disablePreprocessing">
			<visibility>
				<and>
					<objectState name="nature" value="org.eclipse.mtj.core.nature"/>
					<objectState name="nature" value="org.eclipse.mtj.core.preprocessingNature"/>					
	   			</and>
  			</visibility>
         <action
               label="%action.label.DisablePreprocessing"
               class="org.eclipse.mtj.internal.ui.actions.preprocessing.DisablePreprocessingAction"
               menubarPath="org.eclipse.mtj.ui.popup.menu/convert"
               id="org.eclipse.mtj.ui.actions.disablepreprocessing">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.project.enablePreprocessing">
			<visibility>
				<and>
					<objectState name="nature" value="org.eclipse.mtj.core.nature"/>
					<not>
						<objectState name="nature" value="org.eclipse.mtj.core.preprocessingNature"/>					
	   				</not>
	   			</and>
  			</visibility>
         <action
               label="%action.label.EnablePreprocessing"
               class="org.eclipse.mtj.internal.ui.actions.preprocessing.EnablePreprocessingAction"
               menubarPath="org.eclipse.mtj.ui.popup.menu/convert"
               id="org.eclipse.mtj.ui.actions.enablepreprocessing">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.project.enableL10n"
            objectClass="org.eclipse.core.resources.IProject">
         <visibility>
            <and>
               <objectState
                     name="nature"
                     value="org.eclipse.mtj.core.nature">
               </objectState>
               <not>
                  <objectState
                        name="nature"
                        value="org.eclipse.mtj.core.l10nNature">
                  </objectState>
               </not>
            </and>
         </visibility>
         <action
               class="org.eclipse.mtj.internal.ui.actions.l10n.EnableLocalizationAction"
               id="org.eclipse.mtj.ui.actions.enableL10n"
               label="%action.label.Enablel10n"
               menubarPath="org.eclipse.mtj.ui.popup.menu/convert">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="true"
            id="org.eclipse.mtj.ui.action.popup.project.disableL10n"
            objectClass="org.eclipse.core.resources.IProject">
         <visibility>
            <and>
               <objectState
                     name="nature"
                     value="org.eclipse.mtj.core.nature">
               </objectState>
               <objectState
                     name="nature"
                     value="org.eclipse.mtj.core.l10nNature">
               </objectState>
            </and>
         </visibility>
         <action
               class="org.eclipse.mtj.internal.ui.actions.l10n.DisableLocalizationAction"
               id="org.eclipse.mtj.ui.actions.disableL10n"
               label="%action.label.disablel10n"
               menubarPath="org.eclipse.mtj.ui.popup.menu/externalizeGroup">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.eclipse.mtj.ui.localize.strings.classes"
            objectClass="org.eclipse.jdt.core.ICompilationUnit">
         <action
               class="org.eclipse.mtj.internal.ui.actions.l10n.LocalizeStringsAction"
               id="org.eclipse.mtj.ui.source.localize.string.action"
               label="%action.label"
               menubarPath="org.eclipse.jdt.ui.source.menu/externalizeGroup">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.eclipse.mtj.ui.localize.strings.packages"
            objectClass="org.eclipse.jdt.core.IPackageFragment">
         <action
               class="org.eclipse.mtj.internal.ui.actions.l10n.LocalizeStringsAction"
               id="org.eclipse.mtj.ui.source.localize.string.action"
               label="%action.label"
               menubarPath="org.eclipse.jdt.ui.source.menu/externalizeGroup">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.eclipse.mtj.ui.localize.strings.sources"
            objectClass="org.eclipse.jdt.core.IPackageFragmentRoot">
         <action
               class="org.eclipse.mtj.internal.ui.actions.l10n.LocalizeStringsAction"
               id="org.eclipse.mtj.ui.source.localize.string.action"
               label="%action.label"
               menubarPath="org.eclipse.jdt.ui.source.menu/externalizeGroup">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.eclipse.mtj.ui.localize.strings.project"
            objectClass="org.eclipse.jdt.core.IJavaProject">
         <action
               class="org.eclipse.mtj.internal.ui.actions.l10n.LocalizeStringsAction"
               id="org.eclipse.mtj.ui.source.localize.string.action"
               label="%action.label"
               menubarPath="org.eclipse.jdt.ui.source.menu/externalizeGroup">
         </action>
      </objectContribution>
   </extension>
